 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise

 a OR b = Not(Not(a) AND Not(b))
 */

CHIP SimpleOr {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=a, out=nota);
    Nand(a=b, b=b, out=notb);
    Nand(a=nota, b=notb, out=out);
   
}
